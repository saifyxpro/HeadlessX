{
    "name": "headlessx-pro",
    "version": "2.0.0",
    "private": true,
    "packageManager": "pnpm@9.15.0",
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=9.0.0"
    },
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "dev": "concurrently --kill-others \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
        "dev:backend": "cross-env PORT=3001 pnpm --filter headlessx-backend run dev",
        "dev:frontend": "pnpm --filter headlessx-frontend run dev",
        "build": "pnpm -r build",
        "start": "concurrently --kill-others \"pnpm run start:backend\" \"pnpm run start:frontend\"",
        "start:backend": "cross-env PORT=3001 pnpm --filter headlessx-backend run start",
        "start:frontend": "pnpm --filter headlessx-frontend run start",
        "db:push": "pnpm --filter headlessx-backend run db:push",
        "db:migrate": "pnpm --filter headlessx-backend run db:migrate",
        "db:studio": "pnpm --filter headlessx-backend run db:studio",
        "camoufox:fetch": "pnpm --filter headlessx-backend exec npx camoufox-js fetch",
        "install:all": "pnpm install"
    },
    "devDependencies": {
        "concurrently": "^9.1.2",
        "cross-env": "^10.1.0",
        "only-allow": "^1.2.1"
    }
}